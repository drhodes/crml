//#include "./pstdint.h"

typedef signed char int8 ;
typedef unsigned char uint8 ;
typedef signed char int_least8;
typedef unsigned char uint_least8;
typedef signed short int16;
typedef unsigned short uint16;
typedef int16 int_least16;
typedef uint16 uint_least16;
typedef signed long int32;
typedef unsigned long uint32;
typedef int32 int_least32;
typedef uint32 uint_least32;
typedef signed long long int64;
typedef unsigned long long uint64;
typedef int64 int_least64;
typedef uint64 uint_least64;
typedef signed int int_fast8;
typedef unsigned int uint_fast8;
typedef signed int int_fast16;
typedef unsigned int uint_fast16;
typedef signed int int_fast32;
typedef unsigned int uint_fast32;
typedef int_least64 int_fast64;
typedef uint_least64 uint_fast64;

